<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/idContact"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:cardCornerRadius="@dimen/corner_radius_card_contacts"
    android:layout_marginTop="@dimen/paddingGeneral"
    android:layout_marginEnd="@dimen/paddingGeneral"
    android:layout_marginStart="@dimen/paddingGeneral"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="8dp"
        android:padding="8dp">

        <TextView
            android:id="@+id/txtNameContact"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nombre del usuario"
            style="@style/textCardGroups"
            />

    </LinearLayout>


</androidx.cardview.widget.CardView>